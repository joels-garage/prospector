function cookieLoad() {

  var PlantCooky = document.cookie;

  var posit = PlantCooky.indexOf("namserchkey=");
  if (posit != -1) {
    var startKey = posit + 12;
    var endKey = PlantCooky.indexOf(";", startKey);
    if (endKey == -1) endKey = PlantCooky.length;
    var valKey = PlantCooky.substring(startKey, endKey);
    valKey = unescape(valKey);
    var elemKey = document.namserch.mode;
    elemKey.selectedIndex = valKey;
  }

  var posval = PlantCooky.indexOf("namserchval");
  if (posval != -1) {
    var startVal = posval + 12;
    var endVal = PlantCooky.indexOf(";", startVal);
    if (endVal == -1) endVal = document.cookie.length;
    var valVal = PlantCooky.substring(startVal, endVal);
    valVal = unescape(valVal);
    var elemVal = document.namserch.keywordquery;
    elemVal.value = valVal;
  }
}

function addhandlers() {
  document.namserch.mode.onchange = new Function("modeChanged()");
  document.namserch.keywordquery.onchange = new Function("keywordChanged()");
  document.namserch.onsubmit = new Function("keywordChanged()");
}

function modeChanged() {
  var elemVal = document.namserch.mode.selectedIndex;
  document.cookie = "namserchkey=" + escape(elemVal) + ";";
}

function keywordChanged() {
  var elemVal = document.namserch.keywordquery.value;
  document.cookie = "namserchval=" + escape(elemVal) + ";";
}
